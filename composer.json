{
    "name": "study",
    "description": "php study composer",
    "repositories": {
        "codeguy/upload": {
            "type": "vcs",
            "url": "git@github.com:brandonsavage/Upload.git"
        },
        "jaeger/querylist": {
            "type": "vcs",
            "url": "https://github.com/jae-jae/QueryList.git"
        },
        "jaeger/phpquery-single": {
            "type": "vcs",
            "url": "https://github.com/jae-jae/phpQuery-single.git"
        },
        "jaeger/g-http": {
            "type": "vcs",
            "url": "https://github.com/jae-jae/GHttp.git"
        },
        "guzzlehttp/guzzle": {
            "type": "vcs",
            "url": "https://github.com/guzzle/guzzle.git"
        }
    },
    "require": {
        "codeguy/upload": "dev-master",
        "aileshe/upload": "*",
        "league/csv": "^8.2",
        "mk-j/PHP_XLSXWriter": "dev-master",
        "rmccue/requests": "^1.7",
        "rap2hpoutre/fast-excel": "^0.9.0",
        "php-curl-class/php-curl-class": "@dev",
        "noahbuscher/macaw": "dev-master"
    },
    "autoload": {
        "files": [
            "public/helpers.php"
        ],
        "psr-4": {
            "app\\": "App/"
        }
    }

}
